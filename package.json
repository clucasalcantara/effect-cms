{
  "name": "effect-cms",
  "version": "1.0.0",
  "description": "A CMS developed to be magic, and provides you a quickly, automated and innovative interface prototyping, build and deploy proccess",
  "main": "index.js",
  "scripts": {
    "build-server": "webpack --display=errors-only --config webpack/webpack.server.js --watch",
    "build-client": "webpack --display=errors-only --config webpack/webpack.client.js --watch",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev": "npm-run-all --parallel build-* dev:*",
    "graphql": "node ./scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "dependencies": {
    "apisauce": "^0.14.3",
    "auth0-js": "^9.3.3",
    "axios": "0.16.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "chalk": "2.3.2",
    "compression": "1.7.0",
    "concurrently": "3.5.0",
    "dotenv": "5.0.1",
    "dotenv-expand": "4.2.0",
    "dotenv-safe": "5.0.1",
    "express": "4.15.4",
    "express-graphql": "0.6.12",
    "express-http-proxy": "1.0.6",
    "graphql": "0.13.2",
    "jest": "22.4.3",
    "jsdoc": "^3.5.5",
    "lodash": "4.17.4",
    "mongodb": "3.0.4",
    "nodemon": "1.12.0",
    "npm-run-all": "4.1.1",
    "piping": "1.0.0-rc.4",
    "pretty-format": "22.4.3",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "5.0.6",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-saga": "^0.16.0",
    "serialize-javascript": "1.4.0",
    "webpack": "3.5.6",
    "webpack-dev-server": "2.8.2",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "webpack-messages": "^1.0.1"
  }
}
